<h1>EDIT THIS STRIKE</h1>

<%= simple_form_for @strike do |f| %>
  <div class="form-inputs">
    <%= f.input :organization %>
    <%= f.association :category %>
    <%= f.input :description %>
    <%= f.association :union %>
    <%= f.input :start_date %>
    <%= f.input :end_date %>

  </div>
  <div class="form-actions">
    <%= f.submit class: 'btn btn-lg btn-primary' %>
    <%= link_to 'Cancel', root_path, class: 'btn btn-lg btn-secondary' %>
  </div>
<% end %>
